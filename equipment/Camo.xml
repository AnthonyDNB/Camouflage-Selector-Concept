<Equipment>

	#Custom camo/outfits
	<Bind eqp="Flannel_camo">
		<to name="Rangers"/>
		<to name="CIA"/>
	</Bind>

	<Armor name="Flannel_camo" inventoryBinding="MountedGun" unlockCost="0"
			tooltip="@flannel_outfit_name"
			description="@flannel_outfit_desc"
			img="data/models/custom_camo/ranger_flannel_outfit_ui.tga"
		>
		<RenderObject3D  model="data/models/humans/ranger_default_camo_assaulter.khm" diffuseTex="data/models/custom_camo/ranger_flannel_outfit.dds"/>
	</Armor>
	<Bind eqp="DPM_sand_camo">
		<to name="Rangers"/>
		<to name="CIA"/>
	</Bind>
	<Armor name="DPM_sand_camo" inventoryBinding="MountedGun" unlockCost="0"
			tooltip="@DDPM_camo_name"
			description="@DDPM_camo_desc"
			img="data/models/custom_camo/ranger_dpm_desert_camo_ui.tga"
		>
		<MobilityModifiers moveSpeedModifierPercent="-8" turnSpeedModifierPercent="0"/>
		<RenderObject3D model="data/models/humans/ranger_default_camo_assaulter.khm" diffuseTex="data/models/custom_camo/ranger_dpm_camo.dds"/>
		<Params protectionInfoText="@equipment_protect2" suppressedSwitch="Rangers SFLCS Level III">
			<ProtectionArc degrees="90" coveragePercent="40" piercingProtectionLevel="5"/>
			<ProtectionArc degrees="270" coveragePercent="20" piercingProtectionLevel="4"/>
			<ProtectionArc degrees="360" coveragePercent="40" piercingProtectionLevel="5"/>
		</Params>
	</Armor>
</Equipment>
