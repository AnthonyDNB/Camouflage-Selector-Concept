<Equipment>

	#For Debuging
	<Bind eqp="naked_skin">
			<to name="Rangers"/>
			<to name="CIA"/>
	</Bind>
	<ExplosiveCharge name="naked_skin" inventoryBinding="SupportGear2"
				tooltip="@naked_name"
				description="@naked_desc"
				img="data/models/naked_skin_ui.dds"
			>
			<ModifiableParams/>
			<Params>
			</Params>
	</ExplosiveCharge>

	#Default camo for the classes
	<Bind eqp="default_camo_assaulter">
		<to name="Assault"/>

	</Bind>
	<ExplosiveCharge name="default_camo_assaulter" inventoryBinding="SupportGear2"
			tooltip="@MultiCam_camo_name"
			description="@MultiCam_camo_desc"
			img="data/models/basic_armor_ui.dds"
		>
		<RenderObject3D model="data/models/humans/ranger_default_camo_assaulter.khm" diffuseTex="data/models/humans/ranger_default_camo_assaulter.dds"/>
		<ModifiableParams/>
		<Params>
		</Params>
	</ExplosiveCharge>

	<Bind eqp="default_camo_support">
		<to name="Support"/>
	</Bind>
	<ExplosiveCharge name="default_camo_support" inventoryBinding="SupportGear2"
			tooltip="@MultiCam_camo_name"
			description="@MultiCam_camo_desc"
			img="data/models/basic_armor_ui.dds"
		>
		<RenderObject3D model="data/models/humans/ranger_default_camo_support.khm" diffuseTex="data/models/humans/ranger_default_camo_support.dds"/>
		<ModifiableParams/>
		<Params>
		</Params>
	</ExplosiveCharge>

	<Bind eqp="default_camo_marksman">
		<to name="Marksman"/>
	</Bind>
	<ExplosiveCharge name="default_camo_marksman" inventoryBinding="SupportGear2"
			tooltip="@MultiCam_camo_name"
			description="@MultiCam_camo_desc"
			img="data/models/basic_armor_ui.dds"
		>
		<RenderObject3D model="data/models/humans/ranger_default_camo_marksman.khm" diffuseTex="data/models/humans/ranger_default_camo_marksman.dds"/>
		<ModifiableParams/>
		<Params>
		</Params>
	</ExplosiveCharge>

	<Bind eqp="default_camo_grenadier">
		<to name="Grenadier"/>
	</Bind>
	<ExplosiveCharge name="default_camo_grenadier" inventoryBinding="SupportGear2"
			tooltip="@MultiCam_camo_name"
			description="@MultiCam_camo_desc"
			img="data/models/basic_armor_ui.dds"
		>
		<RenderObject3D model="data/models/humans/ranger_default_camo_grenadier.khm" diffuseTex="data/models/humans/ranger_default_camo_grenadier.dds"/>
		<ModifiableParams/>
		<Params>
		</Params>
	</ExplosiveCharge>

	#Custom camo/outfits
	<Bind eqp="Flannel_camo">
		<to name="Rangers"/>
	</Bind>
	<ExplosiveCharge name="Flannel_camo" inventoryBinding="SupportGear2"
			tooltip="@flannel_camo_name"
			description="@flannel_camo_desc"
			img="data/models/basic_armor_ui.dds"
		>
		<RenderObject3D  model="data/models/humans/ranger_default_camo_assaulter.khm" diffuseTex="data/models/custom_camo/ranger_flannel_camo.dds"/>
		<ModifiableParams/>
		<Params>
		</Params>
	</ExplosiveCharge>


	<Bind eqp="Flannel_camo_cia">
		<to name="CIA"/>
	</Bind>
	<LockpickMachine name="Flannel_camo_cia" inventoryBinding="SupportGear2"
		tooltip="@flannel_camo_name"
		description="Testing"
		img="data/models/basic_armor_ui.dds"
		animationSet="generic_device"
		soundUse="lockpick_use"
		>
		<RenderObject3D model="data/models/equipment/tec_torch.khm" skipGOSSAO="true" diffuseTex="data/models/equipment/tec_torch.dds"/>
		<RenderObject3D  model="data/models/humans/ranger_default_camo_assaulter.khm" diffuseTex="data/models/custom_camo/ranger_flannel_camo.dds"/>
		<ModifiableParams
			efficiencyPercent="100"
			changeInTime="500"
			changeOutTime="600"
			audibleSoundRadius="1.5"
		/>
		<MobilityModifiers moveSpeedModifierPercent="-1" turnSpeedModifierPercent="0"/>
	</LockpickMachine>

	<Bind eqp="DPM_sand_camo">
		<to name="Rangers"/>
	</Bind>
	<ExplosiveCharge name="DPM_sand_camo" inventoryBinding="SupportGear2"
			tooltip="@DPM_sand_camo_name"
			description="@DPM_sand_camo_desc"
			img="data/models/basic_armor_ui.dds"
		>
		<RenderObject3D model="data/models/humans/ranger_default_camo_assaulter.khm" diffuseTex="data/models/custom_camo/ranger_dpm_desert_camo.dds"/>
		<ModifiableParams/>
		<Params>
		</Params>
	</ExplosiveCharge>
</Equipment>
